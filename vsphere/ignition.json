{
  "ignition": {
    "config": {},
    "timeouts": {},
    "version": "2.1.0"
  },
  "networkd": {},
  "passwd": {
    "users": [
      {
        "name": "core",
        "sshAuthorizedKeys": [
          "ssh-rsa  sakutz@gmail.com",
          "ssh-rsa  luoh@luoh-m01.vmware.com"
        ]
      }
    ]
  },
  "storage": {
    "files": [
      {
        "filesystem": "root",
        "path": "${hostname_path}",
        "mode": 420,
        "contents": {
          "source": "data:,${hostname}"
        }
      },
      {
        "filesystem": "root",
        "group": {},
        "path": "/etc/cloud-config.yml",
        "user": {},
        "contents": {
          "source": "data:;base64,${cloud_config}",
          "verification": {}
        },
        "mode": 420
      }
    ]
  },
  "systemd": {
    "units": [
      {
        "name": "locksmithd.service",
        "mask": true
      },
      {
        "contents": "[Unit]\nType=oneshot\n\n[Service]\nExecStart=/usr/bin/coreos-cloudinit --from-file /etc/cloud-config.yml\n\n[Install]\nWantedBy=multi-user.target",
        "enabled": true,
        "name": "cloud-init-onboot.service"
      }
    ]
  }
}